class Event:
    def __init__(self, name, no_of_regs):
        self.name = name
        self.num_regs = no_of_regs

    def __unicode__(self):
        return self.name


#| Class that encapsulates information regarding a specific team

class Team:
    def __init__(self, name, event_register):
